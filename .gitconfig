[user]
	email = maximo@mit.edu
	name = Maximo Machado
[init]
	defaultBranch = main
[credential]
	helper = "/mnt/c/Program\\ Files/Git/mingw64/libexec/git-core/git-credential-manager.exe"
[alias]
    lol = "log --graph --oneline --decorate --color --all"
    lols = "log --graph --oneline --decorate --color --all --stat"
    lmao = "log --graph --oneline --decorate --color --all --name-status"
    c = "commit"
    co = "checkout"
    cob = "checkout -b"
    df = "diff --staged"
    dfu = "diff HEAD"
    s = "status"
    undo = "reset HEAD~"

